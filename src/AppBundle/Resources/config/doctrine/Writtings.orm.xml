<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Writtings" table="writtings">
    <indexes>
      <index name="publication_type_FK" columns="publication_type"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="body" type="text" column="body" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="creationDate" type="datetime" column="creation_date" nullable="true"/>
    <field name="modificationDate" type="datetime" column="modification_date" nullable="true"/>
    <many-to-one field="publicationType" target-entity="PublicationsType" fetch="LAZY">
      <join-columns>
        <join-column name="publication_type" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
