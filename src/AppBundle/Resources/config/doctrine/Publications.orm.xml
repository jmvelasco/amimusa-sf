<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Publications" table="publications">
    <indexes>
      <index name="contributor_FK" columns="id_contributor"/>
      <index name="state_FK" columns="id_state"/>
      <index name="writting_FK" columns="id_writting"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idWritting" target-entity="Writtings" fetch="LAZY">
      <join-columns>
        <join-column name="id_writting" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idState" target-entity="States" fetch="LAZY">
      <join-columns>
        <join-column name="id_state" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idContributor" target-entity="Contributors" fetch="LAZY">
      <join-columns>
        <join-column name="id_contributor" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="idMusa" target-entity="Musas" inversed-by="idPublication" fetch="LAZY">
      <join-table name="publications_musas">
        <join-columns>
          <join-column name="id_publication" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="id_musa" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
