<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Contributors" table="contributors">
    <unique-constraints>
      <unique-constraint name="email_UNIQUE" columns="email"/>
      <unique-constraint name="username_UNIQUE" columns="username"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="80" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="16777215" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="linkToProfile" type="string" column="link_to_profile" length="80" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="inscriptionDate" type="datetime" column="inscription_date" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="username" type="string" column="username" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="securityToken" type="string" column="security_token" length="128" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="active" type="boolean" column="active" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
